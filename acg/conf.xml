<?xml version="1.0" encoding="utf-8"?>
<conf>
  <performance> <!--USELESS?!?-->
    <!-- 最长弹幕/像素 -->
    <maxwidth>1</maxwidth>
    <!-- 最高弹幕/像素 -->
    <maxheight>1</maxheight>
    <!-- 表面弹幕容量,包括有特效和无特效 -->
    <maxonstage>1</maxonstage>
    <!-- 特效弹幕容量,如果超出该容量,但是未达到表面弹幕容量,超出部分为无特效 -->
    <maxwitheffect>1</maxwitheffect>
  </performance>
  <server>
    <!-- 使用mukio播放器的方法处理参数,不用改变 -->
    <onhost>yes</onhost>
    <!-- 弹幕加载地址,变量{$id}为弹幕id -->
    <load>get.php?c={$id}</load>
    <!--<load>http://www.bilibili.tv/dm,57896122</load>-->
    <!-- POST发送地址,如果不提供则不发送,变量可用 -->
    <send>add.php</send>
    <!-- Amf的POST发送地址,优先gateway -->
    <gateway></gateway>
  </server>
</conf>